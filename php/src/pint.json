{
    "preset": "laravel",
    "rules": {
        "concat_space": {
            "spacing": "one"
        },
        "not_operator_with_successor_space": false,
        "new_with_parentheses": {
            "anonymous_class": false,
            "named_class": true
        },
        "php_unit_method_casing": false
    }
}
